  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "pnpm build:common && pnpm build:agents && pnpm build:backend && pnpm build:frontend",
    "build:agents": "pnpm --filter \"@llmops-demo-ts/agents\" build",
    "build:backend": "pnpm --filter \"@llmops-demo-ts/backend\" build",
    "build:common": "pnpm --filter \"@llmops-demo/common\" build",
    "build:frontend": "pnpm --filter \"@llmops-demo-ts/frontend\" build",
    "format": "trunk fmt",
    "format:all": "trunk fmt",
    "lint": "eslint --config eslint.config.js",
    "lint:all": "trunk check .",
    "lint:fix": "eslint --config eslint.config.js --fix",
    "lint:security": "trunk check --ci -t=security .",
    "start:backend": "pnpm --filter @llmops-demo-ts/backend dev",
    "start:frontend": "pnpm --filter @llmops-demo-ts/frontend dev",
    "test": "pnpm --filter @llmops-demo-ts/backend test"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "commander": "^14.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.52.3",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  }
}
